{
  "name": "exposr",
  "version": "0.7.1",
  "type": "module",
  "main": "exposr.cjs",
  "bin": "exposr.cjs",
  "author": "Fredrik Lindberg <fli@shapeshifter.se>",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "README",
    "LICENSE",
    "Makefile",
    "Dockerfile",
    "package.json",
    "yarn.lock",
    "build.env",
    "exposr.cjs"
  ],
  "dependencies": {
    "async-sema": "^3.1.1",
    "axios": "^0.21.1",
    "cli-spinners": "^2.6.1",
    "deepmerge": "^4.2.2",
    "dnscache": "^1.0.2",
    "http-parser-js": "^0.5.2",
    "human-readable-ids": "^1.0.4",
    "ink": "^3.2.0",
    "log4js": "^6.4.0",
    "node-emoji": "^1.11.0",
    "port-numbers": "^6.0.0",
    "react": "^17.0.2",
    "ws": "^7.4.6",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "esbuild": "^0.14.23",
    "pkg": "^5.5.2",
    "standard-version": "^9.3.2"
  },
  "scripts": {
    "prepack": "yarn build && yarn run version && yarn bundle",
    "postpack": "rm src/console/components/*.js exposr.cjs",
    "release": "standard-version",
    "build": "babel src/console/components -d src/console/components",
    "version": "scripts/build-version.sh",
    "dev": "babel src/console/components -d src/console/components && ./exposr.sh",
    "bundle": "esbuild --bundle --platform=node --target=node16.14 --minify --analyze --outfile=exposr.cjs exposr.js",
    "dist-linux": "source build.env; pkg -d -o dist/exposr-${EXPOSR_BUILD_VERSION}-linux -t node16-linuxstatic-arm64,node16-linuxstatic-armv7,node16-linuxstatic-x64 exposr.cjs",
    "dist-macos": "source build.env; pkg -d -o dist/exposr-${EXPOSR_BUILD_VERSION}-macos-x64 -t node16-macos-x64 exposr.cjs"
  }
}
